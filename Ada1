		//code for question 5
		var q5 = false;
		for (var i=0; i<quiz.q5.length; i++){
			if (quiz.q5[i].checked) {
				q5 = quiz.q5[i].value;
				break;
			}
		}
		var div = document.getElementById("q5");
		switch (q5){
			case "ant":
				div.innerHTML = "Good design for locating signage."
				div.className = "wrong";
			break;
			case "bee":
				div.innerHTML = "Good design for hardware on accessible doors."
				div.className = "wrong";
			break;
			case "cat":
				div.innerHTML = "Correct answer, wrong design.  Start handrails 12” plus one tread depth before the first riser and continue handrails 12” past top stair riser."
				div.className = "correct";
			break;
			case "dog":
				div.innerHTML = "80” min headroom for obstructions.  Ceiling height should be higher."
				div.className = "wrong";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//code for question 6
		var q6 = false;
		for (var i=0; i<quiz.q6.length; i++){
			if (quiz.q6[i].checked) {
				q6 = quiz.q6[i].value;
				break;
			}
		}
		var div = document.getElementById("q6");
		switch (q6){
			case "ant":
				div.innerHTML = "Correct!  Parking violations exceed 40% of all complaints."
				div.className = "correct";
			break;
			case "bee":
				div.innerHTML = "Restricting service dogs was cited for only 22 out of 9'043 complaints."
				div.className = "wrong";
			break;
			case "cat":
				div.innerHTML = "There was only 1 alleged violation!"
				div.className = "wrong";
			break;
			case "dog":
				div.innerHTML = "Toilet room and bathrooms were the 2nd highest category of complaints."
				div.className = "wrong";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//code for question 7
		var q7 = false;
		for (var i=0; i<quiz.q7.length; i++){
			if (quiz.q7[i].checked) {
				q7 = quiz.q7[i].value;
				break;
			}
		}
		var div = document.getElementById("q7");
		switch (q7){
			case "ant":
				div.innerHTML = "Good start, but there are more strategies."
				div.className = "wrong";
			break;
			case "bee":
				div.innerHTML = "Great idea, but there are more strategies."
				div.className = "wrong";
			break;
			case "cat":
				div.innerHTML = "Fabulous idea, but there are more strategies."
				div.className = "wrong";
			break;
			case "dog":
				div.innerHTML = "Correct!  All of the above are great strategies for CCDA ."
				div.className = "correct";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//code for question 8
		var q8 = false;
		for (var i=0; i<quiz.q8.length; i++){
			if (quiz.q8[i].checked) {
				q8 = quiz.q8[i].value;
				break;
			}
		}
		var div = document.getElementById("q8");
		switch (q8){
			case "ant":
				div.innerHTML = "Good idea, but we're looking for the exception."
				div.className = "wrong";
			break;
			case "bee":
				div.innerHTML = "Correct, but not required!  Transportation is not a communication item anyway."
				div.className = "correct";
			break;
			case "cat":
				div.innerHTML = "Good idea, but wrong.  We're looking for the exception."
				div.className = "wrong";
			break;
			case "dog":
				div.innerHTML = "Good idea, but we're looking for the exception."
				div.className = "wrong";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//code for question 9
		var q9 = false;
		for (var i=0; i<quiz.q9.length; i++){
			if (quiz.q9[i].checked) {
				q9 = quiz.q9[i].value;
				break;
			}
		}
		var div = document.getElementById("q9");
		switch (q9){
			case "ant":
				div.innerHTML = "These activities are all alterations to the facility."
				div.className = "wrong";
			break;
			case "bee":
				div.innerHTML = "These activities are alterations to the facility."
				div.className = "wrong";
			break;
			case "cat":
				div.innerHTML = "Correct!  Normal maintenance is not an alteration to the facilities."
				div.className = "correct";
			break;
			case "dog":
				div.innerHTML = "These activities are all alterations to the facility."
				div.className = "wrong";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//code for question 0
		var q0 = false;
		for (var i=0; i<quiz.q0.length; i++){
			if (quiz.q0[i].checked) {
				q0 = quiz.q0[i].value;
				break;
			}
		}
		var div = document.getElementById("q0");
		switch (q0){
			case "ant":
				div.innerHTML = "Good idea, but there are more."
				div.className = "wrong";
			break;
			case "bee":
				div.innerHTML = "Good idea, but there are more."
				div.className = "wrong";
			break;
			case "cat":
				div.innerHTML = "Good idea, but there are more."
				div.className = "wrong";
			break;
			case "dog":
				div.innerHTML = "Correct!  All of the above modifications could remove barriers to access."
				div.className = "correct";
			break;
			default:
				div.innerHTML = "You did not answer this question."
				div.className = "wrong";
		}

		//end of quiz questions
	};
}
